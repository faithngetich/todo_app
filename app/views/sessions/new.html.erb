<h1>Login</h1>

<%= form_for :session, url: '/login' do |f| %>
  <div>
    <%= f.label :email %> 
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :password %> 
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.label :remember_me %>
    <%= f.check_box :remember_me %>
  </div>
  <div>
    <%= f.submit "Submit" %>
  <div>

<% end %>
